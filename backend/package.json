{
    "name": "warlock_back",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "dev": "npm-run-all --parallel watch:transpile watch:run",
        "watch:run": "nodemon \"./build/warlock.server.js\" --watch \"./build\"",
        "watch:transpile": "DEBUG=socket.io:server webpack --config webpack.config.js --watch",
        "build": "DEBUG=socket.io:server webpack --config webpack.config.js --output-path ./build",
        "start": "node build/warlock.server.js",
        "hello": "rws hello user=$USER",
        "test": "ts-mocha -r tsconfig-paths/register --config ./tests/tsconfig.json 'tests/**/*.test.ts'"
    },
    "main": "src/index.ts",
    "author": "skygate",
    "license": "ISC",
    "dependencies": {
        "@langchain/community": "^0.2.2",
        "@nestjs/microservices": "^10.3.8",
        "@nestjs/platform-socket.io": "^10.3.8",
        "@nestjs/websockets": "^10.3.8",
        "@rws-framework/server": "3.0.0-alpha",
        "@types/xml2js": "^0.4.14",
        "xml2js": "^0.6.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.3.1",
        "@nestjs/schematics": "^10.1.0",
        "@nestjs/testing": "^10.3.2",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/xml2js": "^0.4.14",
        "jest": "^29.7.0"
    }
}
